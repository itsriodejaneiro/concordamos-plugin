<wpml-config>
	<custom-types>
		<custom-type translate="0">vote</custom-type>
	</custom-types>
	<taxonomies>
		<taxonomy translate="0">tag</taxonomy>
	</taxonomies>
	<custom-fields>
		<!-- voting -->
		<custom-field action="ignore">admin_id</custom-field>
		<custom-field action="ignore">credits_voter</custom-field>
		<custom-field action="ignore">date_end</custom-field>
		<custom-field action="ignore">date_start</custom-field>
		<custom-field action="ignore">negative_votes</custom-field>
		<custom-field action="ignore">number_voters</custom-field>
		<custom-field action="ignore">results_end</custom-field>
		<custom-field action="ignore">unique_ids</custom-field>
		<custom-field action="ignore">voting_access</custom-field>
		<custom-field action="ignore">voting_type</custom-field>
		<!-- voting / option-->
		<custom-field action="ignore">original_id</custom-field>
		<!-- option / vote -->
		<custom-field action="ignore">voting_id</custom-field>
		<!-- vote -->
		<custom-field action="ignore">logged_user</custom-field>
		<custom-field action="ignore">unique_id</custom-field>
		<custom-field action="ignore">voting_date</custom-field>
		<custom-field action="ignore">voting_options</custom-field>
	</custom-fields>
</wpml-config>
